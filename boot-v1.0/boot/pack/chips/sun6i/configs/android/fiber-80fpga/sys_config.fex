;A10 PAD application
;---------------------------------------------------------------------------------------------------------
; 说明： 脚本中的字符串区分大小写，用户可以修改"="后面的数值，但是不要修改前面的字符串
; 描述gpio的形式：Port:端口+组内序号<功能分配><内部电阻状态><驱动能力><输出电平状态>
;---------------------------------------------------------------------------------------------------------

[product]
version = "100"
machine = "evb_v12"

[platform]
eraseflag   = 1
;----------------------------------------------------------------------------------
;   system configuration
;   ?
;----------------------------------------------------------------------------------
[target]
boot_clock   		= 1008
dcdc2_vol			= 1400
dcdc3_vol			= 1250
ldo2_vol			= 3000
ldo3_vol			= 2800
ldo4_vol			= 2800
