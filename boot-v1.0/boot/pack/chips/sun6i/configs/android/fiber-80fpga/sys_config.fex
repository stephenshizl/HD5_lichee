[card_boot]
logical_start   	= 40960
sprite_gpio0    	=


[card0_boot_para]
card_ctrl 			= 0
card_high_speed 	= 1
card_line       	= 4
sdc_d1      		= port:PF0<2><1><default><default>
sdc_d0      		= port:PF1<2><1><default><default>
